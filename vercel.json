{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "installCommand": "cd anything/apps/web && npm ci",
  "buildCommand": "npm run build:web",
  "outputDirectory": "anything/apps/web/build/client",
  "functions": {
    "api/_ssr/*.mjs": {
      "includeFiles": "anything/apps/web/build/server/**",
      "maxDuration": 60,
      "regions": ["iad1"]
    }
  },
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/api/_ssr/$1"
    }
  ]
}
