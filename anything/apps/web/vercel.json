{
    "buildCommand": "npm run build",
    "outputDirectory": "build/client",
    "functions": {
        "api/index.mjs": {
            "includeFiles": "build/server/**",
            "maxDuration": 30
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api"
        }
    ]
}